<tr id="<%= dom_id(proponente) %>">
  <td><%= proponente.nome %></td>
  <td><%= proponente.cpf %></td>
  <td><%= proponente.data_nascimento %></td>
  <td><%= proponente.endereco.logradouro %></td>
  <td><%= proponente.endereco.numero %></td>
  <td><%= proponente.endereco.bairro %></td>
  <td><%= proponente.endereco.cidade %></td>
  <td><%= proponente.endereco.estado %></td>
  <td><%= proponente.endereco.cep %></td>
  <td><%= proponente.contato.telefone %></td>
  <td><%= number_to_currency(proponente.salario) %></td>
  <td><%= number_to_currency(proponente.desconto_inss) %></td>
  <td>
    <%= link_to 'Show', proponente, class: 'btn btn-info' %>
    <%= link_to 'Edit', edit_proponente_path(proponente), class: 'btn btn-primary' %>
    <%= link_to 'Delete', proponente, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>
  </td>

</tr>